    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.4",
    "@babel/node": "7.10.5",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/register": "7.10.5",
    "@babel/runtime": "7.11.2",
    "nodemon": "2.0.4",
    "prettier": "2.1.1"
  }
